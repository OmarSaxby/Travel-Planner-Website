        <script>
            // Tab functionality
            document.addEventListener('DOMContentLoaded', function() {
                const tabButtons = document.querySelectorAll('.tab-button');
                const tabContents = document.querySelectorAll('.tab-content');
                tabButtons.forEach(button => {
                    button.addEventListener('click', () => {
                        // Remove active class from all buttons and contents
                        tabButtons.forEach(btn => btn.classList.remove('text-yellow-600', 'border-b-2'));
                        tabButtons.forEach(btn => btn.classList.add('text-gray-600'));
                        tabContents.forEach(content => content.classList.add('hidden'));
                        // Add active class to clicked button
                        button.classList.remove('text-gray-600');
                        button.classList.add('text-yellow-600', 'border-b-2', 'border-yellow-600');
                        // Show corresponding content
                        const tabId = button.id.replace('-tab', '-content');
                        document.getElementById(tabId).classList.remove('hidden');
                    });
                });
                // Real-time price updates (every 30 seconds)
                function updatePrices() {
                    const now = new Date();
                    const timeString = now.getHours().toString().padStart(2, '0') + ':' + 
                                     now.getMinutes().toString().padStart(2, '0') + ':' + 
                                     now.getSeconds().toString().padStart(2, '0');
                    // Update main time display
                    document.getElementById('update-time').textContent = timeString;
                    // Update individual route prices with random fluctuations
                    const route1Price = document.getElementById('route1-price');
                    const route2Price = document.getElementById('route2-price');
                    // Simulate price changes
                    const currentRoute1 = parseFloat(route1Price.textContent.replace('$', ''));
                    const currentRoute2 = parseFloat(route2Price.textContent.replace('$', ''));
                    const change1 = (Math.random() - 0.5) * 100; // -50 to +50 change
                    const change2 = (Math.random() - 0.5) * 50;  // -25 to +25 change
                    const newRoute1 = Math.max(7000, Math.round((currentRoute1 + change1) * 100) / 100);
                    const newRoute2 = Math.max(1800, Math.round((currentRoute2 + change2) * 100) / 100);
                    route1Price.textContent = '$' + newRoute1;
                    route2Price.textContent = '$' + newRoute2;
                    // Update price change indicators
                    const changePercent1 = ((change1 / currentRoute1) * 100).toFixed(1);
                    const changePercent2 = ((change2 / currentRoute2) * 100).toFixed(1);
                    document.querySelector('#route1-price + .price-change').textContent = change1 >= 0 ? `+${changePercent1}%` : `${changePercent1}%`;
                    document.querySelector('#route1-price + .price-change').className = change1 >= 0 ? 'price-change positive' : 'price-change negative';
                    document.querySelector('#route2-price + .price-change').textContent = change2 >= 0 ? `+${changePercent2}%` : `${changePercent2}%`;
                    document.querySelector('#route2-price + .price-change').className = change2 >= 0 ? 'price-change positive' : 'price-change negative';
                    // Update all component prices
                    const components = [
                        { id: 'flight-price', base: 3200, change: (Math.random() - 0.5) * 60 },
                        { id: 'yacht-price', base: 2800, change: (Math.random() - 0.5) * 40 },
                        { id: 'train-price', base: 2450, change: (Math.random() - 0.5) * 80 },
                        { id: 'cargo-flight-price', base: 850, change: (Math.random() - 0.5) * 20 },
                        { id: 'cargo-ship-price', base: 900, change: (Math.random() - 0.5) * 15 },
                        { id: 'freight-train-price', base: 400, change: (Math.random() - 0.5) * 10 }
                    ];
                    components.forEach(comp => {
                        const element = document.getElementById(comp.id);
                        const current = parseFloat(element.textContent.replace('$', ''));
                        const change = (Math.random() - 0.5) * 20;
                        const newValue = Math.max(100, Math.round((current + change) * 100) / 100);
                        element.textContent = '$' + newValue;
                        const changePercent = ((change / current) * 100).toFixed(1);
                        const changeElement = element.nextElementSibling;
                        changeElement.textContent = change >= 0 ? `+${changePercent}%` : `${changePercent}%`;
                        changeElement.className = change >= 0 ? 'price-change positive block mt-1' : 'price-change negative block mt-1';
                    });
                    // Update accommodation prices
                    const accommodationChanges = [
                        { priceId: 'hotel1-price', updateId: 'hotel1-update' },
                        { priceId: 'villa-price', updateId: 'villa-update' }
                    ];
                    accommodationChanges.forEach(acc => {
                        const priceElement = document.getElementById(acc.priceId);
                        const currentPrice = parseFloat(priceElement.textContent.replace('$', ''));
                        const change = (Math.random() - 0.5) * 10;
                        const newPrice = Math.max(100, Math.round((currentPrice + change) * 100) / 100);
                        priceElement.textContent = '$' + newPrice;
                        const changePercent = ((change / currentPrice) * 100).toFixed(1);
                        const changeElement = priceElement.nextElementSibling;
                        changeElement.textContent = change >= 0 ? `+${changePercent}%` : `${changePercent}%`;
                        changeElement.className = change >= 0 ? 'price-change positive ml-2' : 'price-change negative ml-2';
                        // Update timestamp
                        document.getElementById(acc.updateId).textContent = timeString;
                    });
                    // Update service prices
                    const services = ['dining', 'wine', 'fitness', 'security', 'doctor', 'tailoring'];
                    services.forEach(service => {
                        const priceElement = document.getElementById(`${service}-price`);
                        const currentPrice = parseFloat(priceElement.textContent.replace('$', ''));
                        const change = (Math.random() - 0.5) * 5;
                        const newPrice = Math.max(50, Math.round((currentPrice + change) * 100) / 100);
                        priceElement.textContent = '$' + newPrice;
                        const changePercent = ((change / currentPrice) * 100).toFixed(1);
                        const changeElement = priceElement.nextElementSibling;
                        changeElement.textContent = change >= 0 ? `+${changePercent}%` : `${changePercent}%`;
                        changeElement.className = change >= 0 ? 'price-change positive block mt-1' : 'price-change negative block mt-1';
                    });
                    // Update all timestamps
                    document.querySelectorAll('[id$="-update"]').forEach(el => {
                        el.textContent = timeString;
                    });
                }
                // Initial price update
                updatePrices();
                // Update prices every 30 seconds
                setInterval(updatePrices, 30000);
            });
        </script>
    </main>
</body>
</html>
```
